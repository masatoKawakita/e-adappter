$("#conversion_form").html("<%= j(render partial: 'messages/conversion', locals: { conversion: @conversion, conCase: @conCase }) %>")
$('.header__flashMessage').html("<%= j(render 'layouts/notice') %>")

setTimeout(flashNotice, 5000);
function flashNotice(){
  $('.header__flashMessage_type_notice').fadeOut().queue(function() {
    $(this).remove();
  });
}
